<nav class="text-center">
  <ul class="pagination">
    {% for year in years %}
      <li {% if year.year == currentYear %} class="active" {% endif %}>
        <a href="{{ path('home', {'currentYear' : year.year, 'currentMonth' : currentMonth}) }}">
          {{ year.year }}
        </a>
      </li>
    {% endfor %}
  </ul>
</nav>
<nav class="text-center">
  <ul class="pagination">
    {% for month in months %}
      <li {% if month.months == currentMonth%}class="active"{% endif %}>
        <a href="{{ path('home', {'currentYear' : currentYear,'currentMonth' : month.months}) }}">
          {{ month.monthName|date('F') }}
        </a>
      </li>
    {% endfor %}
  </ul>
</nav>
